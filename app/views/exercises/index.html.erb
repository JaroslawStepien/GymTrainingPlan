<p style="color: green"><%= notice %></p>

<h1>Exercises</h1>

<div id="exercises">
  <% @exercises.each do |exercise| %>
    <%= render exercise %>
    <p>
      <%= link_to "Show this exercise", gym_plan_exercise_path(exercise.gym_plan_id, id: exercise) %>
    </p>
  <% end %>
</div>

<%= link_to "New exercise", new_gym_plan_exercise_path %>
